<footer style="padding-bottom:50px;">
  <small>
    <% if current_user %>
      Signed in as  <%= current_user.full_name %> (<%= link_to "Log out", session_path("current"), method: :delete %>)
    <% else %>
      <%= link_to "Log In", new_session_path %> | <%= link_to "Sign Up", new_user_path %>
    <% end %>
    <% if session[:impersonated_user_id] %>
      <% if session[:user_id] != session[:impersonated_user_id] %>
       <%= link_to "Return to yourself" , "admin/change_users/#{session[:impersonated_user_id]}" %>
      <% end %>
      <%= link_to "Admin Panel", admin_users_path %>
    <% end %>
  </small>  
</footer>