<h1>List of all users</h1>
<ul>
<%= @admin_users.each do |user| %>
  <li>
    <strong>Full Name</strong>: <%= user.full_name %><br/>
    <strong>Email</strong>: <%= user.email %><br/>
    <%= link_to "Edit User",  edit_admin_user_path(user) %>
    <%= link_to "Delete User",  admin_user_path(user), method: :delete, confirm: "You sure?" %>
  </li>
<% end %>
</ul>

<%= link_to "Create new user", new_admin_user_path %>